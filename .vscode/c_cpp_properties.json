{
  "configurations": [
    {
      "name": "ESP-IDF",
      "compilerPath": "/usr/bin/xtensa-esp32-elf-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "includePath": [
        "/root/esp/esp-idf/components/**",
        "${workspaceFolder}/firmware/**"
      ],
      "browse": {
        "path": [
          "/root/esp/esp-idf/components",
          "${workspaceFolder}/firmware"
        ],
        "limitSymbolsToIncludedHeaders": false
      },
      "intelliSenseMode": "linux-gcc-x86",
      "compileCommands": "${workspaceFolder}/firmware/build/compile_commands.json",
      "defines": []
    }
  ],
  "version": 4
}
